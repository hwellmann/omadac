<database xmlns="http://omadac.org/xsd/sqldatabase"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xsi:schemaLocation="http://omadac.org/xsd/sqldatabase.xsd sqldatabase.xsd">
  <schema name="engine">
    <table name="target" >
      <description></description>
      <column name="name" type="varchar" length="200" nullable="false" primary_key="true">
        <description></description>
      </column>
      <column name="status" type="varchar" length="10" nullable="false">
        <description></description>
      </column>
    </table>
  </schema>
</database>
